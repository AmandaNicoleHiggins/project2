{{!-- Section categories/accordions --}}

        <div class="accordion" id="accordionExample">
          <div class="card">
            <div class="card-header" id="headingOne">
              <h2 class="mb-0">
                <button class="btn btn-link collapsed text-info" type="button" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                  Movies
                </button>
              </h2>
            </div>

            <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordionExample">
              <div class="card-body">
                <div class="form-group input-group">

                  <label id="movie-label" for="movie-text" class="w-100">Search for a Movie: </label>
                  <input type="text" id="movie-text" class="form-control" aria-describedby="example-text" placeholder="Movie Title">
                  <span class="input-group-btn">
                    <button id="submitMovie" class="btn btn-info float-right">Submit</button>
                  </span>
                </div>
                <ul id="movie-list" class="list-unstyled">
                </ul>
              </div>
            </div>
          </div>

          <div class="card">
            <div class="card-header" id="headingTwo">
              <h2 class="mb-0">
                <button class="btn btn-link collapsed text-info" type="button" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                  Books
                </button>
              </h2>
            </div>
            <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionExample">
              <div class="card-body">
                <div class="form-group input-group">
                  <label id="book-label" for="book-text" class="w-100">Search for a Book: </label>
                  <input type="text" id="book-text" class="form-control" aria-describedby="example-text" placeholder="Book Title">
                  <span class="input-group-btn">
                    <button id="submitBook" class="btn btn-info float-right">Submit</button>
                  </span>
                </div>
                <ul id="book-list" class="list-unstyled">
        
                </ul>
              
              </div>
            </div>
          </div>

          <div class="card">
            <div class="card-header" id="headingThree">
              <h2 class="mb-0">
              
                <button class="btn btn-link collapsed text-info" type="button" data-toggle="collapse" data-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                  Video Games

                </button>
              </h2>
            </div>
            <div id="collapseThree" class="collapse" aria-labelledby="headingThree" data-parent="#accordionExample">
              <div class="card-body">
                <div class="form-group input-group">
                  <label id="game-label" for="game-text" class="w-100">Search for a Game: </label>
                  <input type="text" id="game-text" class="form-control" aria-describedby="example-text" placeholder="Video Game Title">
                  <span class="input-group-btn">
                    <button id="submitGame" class="btn btn-info float-right">Submit</button>
                  </span>
                </div>
                <ul id="game-list" class="list-unstyled">
        
                </ul>
              </div>
            </div>





          </div>
        </div>
      </div>

      <div class="card">
            <div class="card-header" id="headingFour">
              <h2 class="mb-0">
              
                <button class="btn btn-link collapsed" type="button" data-toggle="collapse" data-target="#collapseFour" aria-expanded="false" aria-controls="collapseFour">
                 Songs

                </button>
              </h2>
            </div>
            <div id="collapseFour" class="collapse" aria-labelledby="headingFour" data-parent="#accordionExample">
              <div class="card-body">
                <div class="form-group input-group">
                  <label id="song-label" for="song-text" class="w-100">Search for a Song: </label>
                  <input type="text" id="song-text" class="form-control" aria-describedby="song-text" placeholder="Song, artist, or album">
                  <span class="input-group-btn">
                    <button id="submitSong" class="btn btn-primary float-right">Submit</button>
                  </span>
                </div>
                <ul id="song-list" class="list-unstyled">
        
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="card">
            <div class="card-header" id="headingFive">
              <h2 class="mb-0">
              
                <button class="btn btn-link collapsed" type="button" data-toggle="collapse" data-target="#collapseFive" aria-expanded="false" aria-controls="collapseFive">
                  Self-Care

                </button>
              </h2>
            </div>
            <div id="collapseFive" class="collapse" aria-labelledby="headingFive" data-parent="#accordionExample">
              <div class="card-body">
                <div class="form-group input-group">
                  <label id="self-label" for="self-text" class="w-100">Add whatever you need to do to take care of yourself!</label>
                  <input type="text" id="self-text" class="form-control" aria-describedby="self-text" placeholder="take a long bath">
                  <span class="input-group-btn">
                    <button id="submitSelf" class="btn btn-primary float-right">Submit</button>
                  </span>
                </div>
                <ul id="self-list" class="list-unstyled">
        
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>




    </div>

    <div class="accordion" id="accordionExample">
      <div class="card">
        <div class="card-header" id="headingTwo">
